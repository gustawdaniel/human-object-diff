{
  "name": "human-object-diff",
  "description": "Human Readable Difference Between Two Objects",
  "version": "2.0.2",
  "author": "Spencer Snyder <sasnyde2@gmail.com> (https://spencersnyder.io/)",
  "bugs": {
    "url": "https://github.com/Spence-S/human-object-diff/issues",
    "email": "sasnyde2@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Spencer Snyder <sasnyde2@gmail.com> (https://spencersnyder.io/)"
  ],
  "dependencies": {
    "date-fns": "^2.29.3",
    "deep-diff": "^1.0.2",
    "fast-deep-equal": "^3.1.3",
    "humanize-string": "^2.1.0",
    "titleize": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/deep-diff": "^1.0.2",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "codecov": "^3.8.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.14",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.36.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-lass": "^2.0.1",
    "eslint-config-xo-typescript": "^0.56.0",
    "fixpack": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "xo": "^0.53.1"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "homepage": "https://github.com/Spence-S/human-object-diff",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "deep-diff",
    "human-readable",
    "humanize",
    "object-diff"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "xo --fix"
    ],
    "package.json": [
      "fixpack"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Spence-S/human-object-diff"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "prettier --write . --ignore-path .gitignore && DEBUG=eslint:cli-engine xo && DEBUG=eslint:cli-engine npx eslint --ext .ts src --fix",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "jest",
    "test-coverage": "yarn run lint && yarn run nyc",
    "build": "npx esbuild --bundle src/index.ts --outdir=cjs --platform=node --format=cjs && echo '{\"type\": \"commonjs\"}' > cjs/package.json && npx esbuild --bundle src/index.ts --outdir=esm --platform=neutral --format=esm && echo '{\"type\": \"module\"}' > esm/package.json"
  },
  "exports": {
    "require": "./cjs/index.js",
    "import": "./esm/index.js"
  },
  "main": "./cjs/index.js",
  "types": "./src",
  "overrides": {
    "decamelize": "4.0.0"
  }
}
