{
  "name": "human-object-diff",
  "description": "Human Readable Difference Between Two Objects",
  "version": "2.0.2",
  "author": "Spencer Snyder <sasnyde2@gmail.com> (https://spencersnyder.io/)",
  "bugs": {
    "url": "https://github.com/Spence-S/human-object-diff/issues",
    "email": "sasnyde2@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Spencer Snyder <sasnyde2@gmail.com> (https://spencersnyder.io/)"
  ],
  "dependencies": {
    "date-fns": "^2.29.3",
    "deep-diff": "^1.0.2",
    "fast-deep-equal": "^3.1.3",
    "humanize-string": "^2.1.0",
    "titleize": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "ava": "^5.2.0",
    "codecov": "^3.8.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-xo-lass": "^2.0.1",
    "xo": "^0.53.1",
    "fixpack": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "nyc": "^15.1.0"
  },
  "engines": {
    "node": ">=8.3"
  },
  "homepage": "https://github.com/Spence-S/human-object-diff",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "deep-diff",
    "human-readable",
    "humanize",
    "object-diff"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "xo --fix"
    ],
    "package.json": [
      "fixpack"
    ]
  },
  "main": "src/index.js",
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Spence-S/human-object-diff"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "xo",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "yarn run lint && yarn run ava",
    "test-coverage": "yarn run lint && yarn run nyc"
  }
}
